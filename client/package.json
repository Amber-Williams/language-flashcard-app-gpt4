  "author": "amber williams <https://github.com/amber-williams>",
  "keywords": [
    "react",
    "typescript",
    "starter",
    "vite",
    "material-ui",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amber-williams/language-flashcard-app-gpt4.git"
  },
  "bugs": {
    "url": "https://github.com/amber-williams/language-flashcard-app-gpt4/issues"
  },
  "homepage": "https://github.com/amber-williams",
  "scripts": {
    "dev": "vite --open",
    "start": "vite --open",
    "host": "vite --host 0.0.0.0 --port 8008",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss}\" --write"
  },
  "dependencies": {
    "@mb3r/component-library": "1.2.0",
    "random-words": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
